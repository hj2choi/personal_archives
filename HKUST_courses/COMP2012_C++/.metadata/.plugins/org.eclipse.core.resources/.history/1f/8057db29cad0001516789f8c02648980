#include <iostream>

using namespace std;

int main()
{
	int x, y, z;

	x = 5;
	y = 6;
	z = x+y;

	cout <<"The sum of "<< x << " and " << y << " is " << z << endl;
	cout <<"The product of "<< x << " and "<< y << " is "<< x*y << endl;

	return 0;
}



