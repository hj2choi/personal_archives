#include "stringset.h"



StringSet::StringSet()
{
	size=0;
	capacity=0;
	arr=NULL;
}

StringSet::~StringSet()
{
	delete arr;
	arr=0;
}

StringSet::StringSet(const std::string& oneItem)
{
	size=1;
	capacity=2;
	arr = new std::string[capacity];
	arr[0].std::strcpy(oneItem);

}
