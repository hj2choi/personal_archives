public class pr030ProbSolving62
{	public static void main (String []args)
	{
		java.util.Scanner s = new java.util.Scanner(System.in);
		System.out.println("Determine the length");
		int a = s.nextInt();
		a=5;
		for (int i = 0;i<a ;i++ )
		{
			for (int b = 0;b<i ;b++ )
				System.out.print("*");
			System.out.println("");
		}
		for (int x = a;x>0 ;x-- )
		{
			for (int y = x;y>0 ;y-- )
				System.out.print("*");
			System.out.println("");
		}
	}
}
